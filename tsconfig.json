{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["@types/styled-components-react-native"],
    "paths": {
      "*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@layout/*": ["src/layout/*"],
      "env": ["src/config/env.ts"] // 'env' 모듈에 대한 경로 추가
    }
  }
}
